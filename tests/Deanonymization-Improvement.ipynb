    "import itertools\n",
    "import random\n",
    "import numpy as np\n",
    "from scipy.spatial import distance\n",
    "\n",
    "from scipy.spatial.distance import cdist\n",
    "from scipy.optimize import linear_sum_assignment"
   ]
